from google.colab import drive
drive.mount('/content/drive')

import zipfile
import os


# Path to the zip file
zip_path = ''

# Path to extract the contents
extract_path = ''

# Create the directory if it doesn't exist
os.makedirs(extract_path, exist_ok=True)

# Unzip the file
with zipfile.ZipFile(zip_path, 'r') as zip_ref:
    zip_ref.extractall(extract_path)

print("Files extracted to:", extract_path)


import pickle

# Path to your .pkl file
file_path = '_dataset.pkl'

# Open the file in binary mode
with open(file_path, 'rb') as file:
    # Load the object from the pickle file
    data = pickle.load(file)

# Now 'data' contains the object from the pickle file

# Define the path to your .rar file
rar_file_path = '.rar'  # Replace with your .rar file path
extract_path = ''  # Replace with your desired extraction path

# Create the extraction directory if it does not exist
os.makedirs(extract_path, exist_ok=True)

# Extract the .rar file
!unrar x -r "{rar_file_path}" "{extract_path}"

drive.flush_and_unmount()
